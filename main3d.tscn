code = "shader_type canvas_item;

uniform vec4 color_0 = vec4(0, 0, 0, 1);
uniform vec4 color_1 = vec4(0.25, 0.25, 0.25, 1);
uniform vec4 color_2 = vec4(0.5, 0.5, 0.5, 1);
uniform vec4 color_3 = vec4(0.75, 0.75, 0.75, 1);
uniform vec4 color_4 = vec4(1, 1, 1, 1);

float to_greyscale(vec4 colour)
{
	//return (colour.r + colour.g + colour.b) / 3.0;
	return 0.2126 * colour.r + 0.7152 * colour.g + 0.0722 * colour.b;
}

void fragment()
{
	vec4 col = texture(SCREEN_TEXTURE, SCREEN_UV);
	float greyscale = to_greyscale(col);
	COLOR = greyscale > 0.875 ? color_4 : (greyscale > 0.625 ? color_3 : (greyscale > 0.375 ? color_2 : ( greyscale > 0.125  ? color_1: color_0)));
}"
